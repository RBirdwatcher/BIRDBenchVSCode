{
    "name": "regdna-editor",
    "displayName": "Regdna Editor",
    "description": "Regdna Editor",
    "version": "1.",
    "publisher": "BirdSoftwareSolutions",
    "icon": "images/BirdSoftwareSolutions.png",
    "license": "EPL-1.0",
    "repository": {
        "type": "git",
        "url": "http://www.BirdSoftwareSolutions.com"
    },
    "bugs": {
        "url": "http://www.BirdSoftwareSolutions.com"
    },
    "engines": {
        "vscode": "^1.67.1"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:Regdna"
    ],
    "main": "out/extension",
    "contributes": {
        "languages": [
            {
                "id": "Regdna",
                "aliases": [
                    "Regdna"
                ],
                "extensions": [
                    ".regdna"
                ],
                "configuration": "./Regdna.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "Regdna",
                "scopeName": "text.Regdna",
                "path": "./syntaxes/Regdna.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "Regdna.a.proxy",
                "title": "Regdna Command A"
            },
            {
                "command": "Regdna.b",
                "title": "Regdna Command B"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^17.0.34",
        "@types/vscode": "^1.67.0",
        "typescript": "^4.6.4",
        "@vscode/test-electron": "^2.1.3"
    },
    "dependencies": {
        "vscode-languageclient": "^8.0.1"
    },
    "scripts": {
        "prepublish": "tsc -p ./src",
        "compile": "tsc -p ./src",
        "watch": "tsc -w -p ./src",
        "update-vscode": "node ./node_modules/vscode/bin/install"
    }
}
