{
    "name": "ecore4reg-editor",
    "displayName": "Ecore4Reg Editor (Beta)",
    "description": "XCore with Lineage Extensions editor (Beta)",
    "version": "0.9.6",
    "publisher": "BirdSoftwareSolutions",
    "icon": "images/BirdSoftwareSolutions.png",
    "license": "EPL-1.0",
    "repository": {
        "type": "git",
        "url": "http://www.BirdSoftwareSolutions.com"
    },
    "bugs": {
        "url": "http://www.BirdSoftwareSolutions.com"
    },
    "engines": {
        "vscode": "^1.67.1"
    },
    "categories": [
        "Programming Languages"
    ],
    "activationEvents": [
        "onLanguage:Ecore4Reg"
    ],
    "main": "out/extension",
    "contributes": {
        "languages": [
            {
                "id": "Ecore4Reg",
                "aliases": [
                    "Ecore4Reg"
                ],
                "extensions": [
                    ".ecore4reg"
                ],
                "configuration": "./Ecore4Reg.configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "Ecore4Reg",
                "scopeName": "text.Ecore4Reg",
                "path": "./syntaxes/Ecore4Reg.tmLanguage.json"
            }
        ],
        "commands": [
            {
                "command": "Ecore4Reg.a.proxy",
                "title": "Ecore4Reg Command A"
            },
            {
                "command": "Ecore4Reg.b",
                "title": "Ecore4Reg Command B"
            }
        ]
    },
    "devDependencies": {
        "@types/node": "^17.0.34",
        "@types/vscode": "^1.67.0",
        "typescript": "^4.6.4",
        "@vscode/test-electron": "^2.1.3"
    },
    "dependencies": {
        "vscode-languageclient": "^8.0.1"
    },
    "scripts": {
        "prepublish": "tsc -p ./src",
        "compile": "tsc -p ./src",
        "watch": "tsc -w -p ./src",
        "update-vscode": "node ./node_modules/vscode/bin/install"
    }
}
